	$(INSTALL_PROG) $(top_builddir)/examples/cvodes/serial/cvsadjbanx          $(EXS_INSTDIR)/cvodes/serial/
	$(INSTALL_PROG) $(top_builddir)/examples/cvodes/serial/cvsadjdenx          $(EXS_INSTDIR)/cvodes/serial/
