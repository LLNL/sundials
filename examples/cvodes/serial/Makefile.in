SHELL = @SHELL@

srcdir       = @srcdir@
top_builddir = @top_builddir@
builddir     = @builddir@
abs_builddir = @abs_builddir@
prefix       = @prefix@
exec_prefix  = @exec_prefix@
includedir   = @includedir@
libdir       = @libdir@

INSTALL        = @INSTALL@
INSTALL_PROG   = @INSTALL_PROGRAM@
INSTALL_FILE   = @INSTALL_DATA@

LIBTOOL      = @LIBTOOL@
LIBTOOL_DEPS = @LIBTOOL_DEPS@

CPP      = @CPP@
CPPFLAGS = @CPPFLAGS@
CC       = @CC@
CFLAGS   = @CFLAGS@
LDFLAGS  = @LDFLAGS@
LIBS     = @LIBS@

OBJ_EXT = @OBJ_EXT@

EXS_INSTDIR = @EXS_INSTDIR@

top_srcdir = $(srcdir)/../../..
SUNDIALS_INCDIR = $(top_srcdir)/include
SOLVER_SRCDIR   = $(top_builddir)/src/cvodes
NVEC_SRCDIR     = $(top_builddir)/src/nvec_ser

SUNDIALS_INCS = -I$(SUNDIALS_INCDIR) -I$(top_builddir)/src
SUNDIALS_LIBS = $(SOLVER_SRCDIR)/libsundials_cvodes.la $(NVEC_SRCDIR)/libsundials_nvecserial.la

EXEC_FILES = cvsdenx cvsdenx_uw cvsbanx cvskryx cvskryx_bp cvsdirectdem cvskrydem_pre cvsfwdnonx cvsfwddenx cvsfwdkryx cvsadjdenx cvsadjbanx cvsadjkryx_int cvsadjkryx_pnt cvskrydem_lin
mkinstalldirs = $(SHELL) $(top_srcdir)/config/mkinstalldirs

all: $(EXEC_FILES)

cvsdenx: $(srcdir)/cvsdenx.c
	$(LIBTOOL) --mode=compile $(CC) $(CPPFLAGS) $(SUNDIALS_INCS) $(CFLAGS) -c $(srcdir)/cvsdenx.c
	$(LIBTOOL) --mode=link $(CC) -o $(builddir)/cvsdenx $(builddir)/cvsdenx.$(OBJ_EXT) $(CFLAGS) $(LDFLAGS) $(SUNDIALS_LIBS) $(LIBS)
