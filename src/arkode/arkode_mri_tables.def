    B->q=2;
    B->p=1;

    B->c[1] = ONE;
    B->c[2] = ONE;

    B->A[1][0] = ONE;
    B->A[2][0] = SUN_RCONST(0.5);
    B->A[2][2] = SUN_RCONST(0.5);

    B->b[0] = SUN_RCONST(0.5);
    B->b[2] = SUN_RCONST(0.5);

    B->d[2] = SUN_RCONST(1.0);

    C = MRIStepCoupling_MIStoMRI(B, B->q, B->p);
    ARKodeButcherTable_Free(B);
    return C;
  })

