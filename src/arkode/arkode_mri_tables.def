ARK_MRI_TABLE(ARKODE_MRI_GARK_ERK33a, { /* A. Sandu, SINUM 57:2300-2327, 2019 */
    MRIStepCoupling C = MRIStepCoupling_Alloc(2, 4, MRISTEP_EXPLICIT);

    C->q = 3;
    C->p = 2;

    C->c[1] = ONE/SUN_RCONST(3.0);
    C->c[2] = TWO/SUN_RCONST(3.0);
    C->c[3] = ONE;

    C->W[0][1][0] =  ONE/SUN_RCONST(3.0);
    C->W[0][2][0] = -ONE/SUN_RCONST(3.0);
    C->W[0][2][1] =  TWO/SUN_RCONST(3.0);
    C->W[0][3][1] = -TWO/SUN_RCONST(3.0);
    C->W[0][3][2] =  ONE;
    C->W[0][4][0] =  ONE/SUN_RCONST(12.0);
    C->W[0][4][1] = -ONE/SUN_RCONST(3.0);
    C->W[0][4][2] =  SUN_RCONST(7.0)/SUN_RCONST(12.0);

    C->W[1][3][0] =  ONE/TWO;
    C->W[1][3][2] = -ONE/TWO;
    return C;
  })

