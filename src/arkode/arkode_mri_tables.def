    C->q = 3;
    C->p = 0;

    C->c[1] = ONE/SUN_RCONST(3.0);
    C->c[2] = TWO/SUN_RCONST(3.0);
    C->c[3] = ONE;

    C->W[0][1][0] =  ONE/SUN_RCONST(3.0);
    C->W[0][2][0] = -ONE/SUN_RCONST(3.0);
    C->W[0][2][1] =  TWO/SUN_RCONST(3.0);
    C->W[0][3][1] = -TWO/SUN_RCONST(3.0);
    C->W[0][3][2] =  ONE;

